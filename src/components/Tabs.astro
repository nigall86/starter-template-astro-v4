---
// data imports
import tabsData from '@data/tabsData.json';
---

<div class='tabs'>
  <ul class='tabs__header'>
    {tabsData.map((item) => <li> {item.heading} </li>)}
  </ul>
  <ul class='tabs__content'>
    {tabsData.map((item) => <li class='tab'>{item.content}</li>)}
  </ul>
</div>

<script>
  import '@scripts/tabs';
</script>
